<% @teachers.last(5).each do | teacher | %>
  <% if teacher.reviews.present? %>
    <%= teacher.reviews.average(:rating).round(2) %>
  <%else %>
No reviews yet
  <% end %>
<% if teacher.image_url.present? %>
  <%= link_to image_tag(teacher.image_url.to_s, height: 100, width: 100), teacher %>
<%else %>
  <%= link_to image_tag("http://placehold.it/370x210", height: 100, width: 100), teacher %>
<% end %>
    <%= link_to teacher.name, teacher  %>
<% end %>
